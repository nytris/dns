{
    "name": "nytris/dns",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytris\\Dns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Dns\\Tests\\": "tests/"
        }
